{
  "name": "three-interact",
  "version": "0.1.1",
  "description": "Easy Three.js interactions with declarative method chaining",
  "type": "module",
  "main": "./dist/three-interact.umd.js",
  "module": "./dist/three-interact.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/three-interact.es.js",
      "require": "./dist/three-interact.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/roukara/three-interact.git"
  },
  "homepage": "https://github.com/roukara/three-interact",
  "bugs": {
    "url": "https://github.com/roukara/three-interact/issues"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "examples": "vite examples",
    "lint": "eslint src tests examples --ext .ts,.js",
    "lint:fix": "eslint src tests examples --ext .ts,.js --fix"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.24.0",
    "@testing-library/dom": "^10.4.0",
    "@types/node": "^22.14.1",
    "@types/three": "^0.175.0",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^9.24.0",
    "jsdom": "^26.1.0",
    "typescript": "~5.7.2",
    "vite": "^6.3.0",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "three": ">=0.130.0"
  },
  "keywords": [
    "three",
    "three.js",
    "interaction",
    "events",
    "declarative",
    "webgl"
  ],
  "author": "roukara",
  "license": "MIT"
}
